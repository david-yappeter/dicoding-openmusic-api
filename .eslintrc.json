{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "object-curly-spacing": 0,
        "object-curly-newline": 0,
        "max-len": 0,
        "indent": 0,
        "comma-dangle": 0,
        "linebreak-style": 0,
        "space-before-function-paren": 0
    }
}
